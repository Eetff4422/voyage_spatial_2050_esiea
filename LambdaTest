        stage('HELLO'){
            steps{
                echo 'Hello World'
            }
        }
        stage('BUILD'){
            steps{
                echo 'Development'
            }
        }
        stage('Test'){
            steps{
                echo 'Testing'
            }
        }
        stage('DEPLOY'){
            steps {
                echo 'Mise en production'
            }
        }
    }
}
